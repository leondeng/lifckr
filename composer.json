{
  "name": "lifckr",
  "version": "1.0.0",
  "keywords": [
    "Flickr"
  ],

  "homepage": "https://github.com/leondeng/lifckr",
  "license": "MIT",

  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "4.6.*"
  },
  "autoload": {
    "psr-0": {
      "FAN\\LIFCKR\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "FAN\\LIFCKR\\Tests": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "scripts": {
    "test:unit": "php vendor/bin/phpunit --bootstrap tests/bootstrap.php tests/FAN/LIFCKR/Tests/unit"
  }
}
